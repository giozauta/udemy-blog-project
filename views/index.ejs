<%- include("partials/header.ejs") %>
<%- include("partials/navbar.ejs") %>


<div class="blog_image_container">
   <img src="/images/blogcover.webp" style="object-fit:cover;" width="100%" height="400px" alt="Study Tools">
   <div class="blog_image_text">
      <p>Travel Blog</p>
      <h1>Going Places</h1>
      <p>I haven't been everywhere, but it's on my list</p>
   </div>
</div>

<div class="paragrap_box">
   <p>
      I'm a paragraph. Click here to add your own text and edit me. It’s easy.
      Just click “Edit Text” or double click me to add your own content and make changes to the font.
   </p>
</div>

<div class="blog_typ_boxes">
      <div class="travel">
         <div><a><p>Travel</p></a></div>
      </div>
      <div class="eat">
         <div><a><p>Eat</p></a></div>
      </div>
      <div class="relax">
         <div><a><p>Relax</p></a></div>
      </div>
</div>

<div class="as_featured_in">
   <div class="as_featured_in_img"></div>
</div>




<% data.forEach(element => { %>
   <div class="blog_box">
      <img src="<%= element.url %>" alt="">
      <div class="blog_box_content">
         <h3><%= element.name %></h3>
         <p><%= element.post %></p> 
      </div>
   </div>
<% }); %>



<%- include("partials/footer.ejs") %>
